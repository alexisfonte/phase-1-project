            },
            {
                "ingredient_id": "3",
                "ingredient": "large eggs",
                "amount": "2",
                "unit": "each"
            },
            {
                "ingredient_id": "4",
                "ingredient": "vanilla extract",
                "amount": "1",
                "unit": "tsp"
            },
            {
                "ingredient_id": "5",
                "ingredient": "all-purpose flour",
                "amount": "2",
                "unit": "cups"
            },
            {
                "ingredient_id": "6",
                "ingredient": "granulated sugar",
                "amount": "1",
                "unit": "cup"
            },
            {
                "ingredient_id": "7",
                "ingredient": "baking soda",
                "amount": "1",
                "unit": "tsp"
            },
            {
                "ingredient_id": "8",
                "ingredient": "salt",
                "amount": "1/2",
                "unit": "tsp"
            }
        ],
        "instructions": [
            {
                "instruction_id": "1",
                "instruction": "Preheat oven to 350 degrees. Spray a loaf pan with non-stick cooking spray or grease with butter and set aside."
            },
            {
                "instruction_id": "2",
                "instruction": "In a large bowl, combine melted butter and bananas and mash with a fork."
            },
            {
                "instruction_id": "3",
                "instruction": "Add the vanilla extract and egg to the bowl and use the same fork to mash and stir until no yellow streaks of egg remain."
            },
            {
                "instruction_id": "4",
                "instruction": "Pour in the melted chocolate into the egg mixture, along with the flour. Use a rubber spatula and fold it in until fully combined. The batter will be thick but should not be lumpy."
            },
            {
                "instruction_id": "5",
                "instruction": "In a second large bowl whisk together the flour, sugar, baking soda, and salt."
            },
            {
                "instruction_id": "6",
                "instruction": "Add the dry ingredients to the wet ingredients and mix together with a spatula just until combined. Do not overmix."
            },
            {
                "instruction_id": "7",
                "instruction": "Pour the batter into prepared loaf pan and bake for 45-55 minutes until a toothpick inserted in the center of the bread comes out clean."
            }
        ]
