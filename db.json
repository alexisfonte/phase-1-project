            },
            {
                "ingredient_id": "10",
                "ingredient": "fresh blueberries",
                "amount": "1 1/2",
                "unit": "cups"
            }
        ],
        "instructions": [
            {
                "instruction_id": "1",
                "instruction": "Preheat the oven to 350 F. Lightly grease a 9x5-inch loaf pan with cooking spray oil."
            },
            {
                "instruction_id": "2",
                "instruction": "In a large mixing bowl, combine the dry ingredients including flour, baking powder, sugar, and salt. Whisk together until evenly distributed and set aside."
            },
            {
                "instruction_id": "3",
                "instruction": "In a medium mixing bowl, whisk well to combine the wet ingredients including milk, oil, egg, vanilla extract, lemon juice, and lemon zest."
            },
            {
                "instruction_id": "4",
                "instruction": "Pour the wet ingredients into the dry ingredients and mix together using a rubber spatula until just combined and no dry ingredients are visible. Do not overmix (overmixing will make the bread denser and firmer). Fold in 1 cup of blueberries until evenly mixed."
            },
            {
                "instruction_id": "5",
                "instruction": "Pour the batter into the prepared loaf pan and add the remaining ½ cup blueberry on top."
            },
            {
                "instruction_id": "6",
                "instruction": "Bake for 55 minutes until golden brown and a toothpick inserted into the center of the cake comes out clean. Let cool for 10-15 minutes, then invert to remove the loaf."
            },
            {
                "instruction_id": "7",
                "instruction": "Serve warm or at room temperature. Drizzle with glaze if desired."
            }
